<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/on_boarding_navigation"
    app:startDestination="@id/loginFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.store.storekhata.Login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_trackYourDebtFragment"
            app:destination="@id/trackYourDebtFragment" />
    </fragment>
    <fragment
        android:id="@+id/trackYourDebtFragment"
        android:name="com.store.storekhata.TrackDebit.TrackYourDebtFragment"
        android:label="fragment_track_your_debt"
        tools:layout="@layout/fragment_track_your_debt" />
</navigation>